{
  "name": "workout-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.8",
    "@vercel/analytics": "^1.0.0",
    "@vueuse/core": "^10.1.0",
    "@vueuse/head": "^1.1.26",
    "daisyui": "^2.51.6",
    "dayjs": "^1.11.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-demi": "^0.14.0",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@dochner/eslint-config": "^0.0.4",
    "@iconify-json/bx": "^1.1.6",
    "@iconify-json/mdi": "^1.1.50",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.51.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/ui": "^0.30.1",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "https-localhost": "^4.7.1",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.3.1",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.9.2",
    "typescript": "^5.0.4",
    "unocss": "^0.51.8",
    "unocss-preset-daisy": "^4.0.0",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^2.0.0",
    "vite": "^4.3.3",
    "vite-bundle-visualizer": "^0.6.0",
    "vite-plugin-inspect": "^0.7.24",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-vue-inspector": "^3.4.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-webfont-dl": "^3.7.3",
    "vite-ssg": "^0.22.2",
    "vite-ssg-sitemap": "^0.4.3",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.4.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@dochner",
    "rules": {
      "vue/no-v-text-v-html-on-component": "off"
    }
  }
}
